@using EdpsProjectManagement.Web.HtmlExtensions
@using EdpsProjectManagement.Entities

@model List<EdpsProjectManagement.Entities.BusinessEntities.Task>
@{    ViewBag.Title = "Histories";    Layout  = "~/Views/Shared/_Layout.cshtml";<!--add customized code between this region--><!--add customized code between this region-->}<div class="m_title_box">    <ol>        <li data-id="Task">            <a href="#">Task</a><i class="icon-arrowright"></i>        </li>        <li data-id="Histories">Histories</li>    </ol></div><div class="panel panel-default">        <div class="panel-heading">Task</div>    <div class="panel-body">    <div class="table-responsive">        <table class="table table-striped table-bordered table-hover">            <thead>            <tr>                <th class="text-left">Name</th>
                <th class="text-left">Iteration</th>
                <th class="text-left">Project</th>
                <th class="text-left">Repository</th>
                <th class="text-left">ActualHours</th>
                <th class="text-left">Assignee</th>
                <th class="text-left">EstimateHours</th>
                <th class="text-left">ImportDateTime</th>
                <th class="text-left">Label</th>
                <th class="text-left">ReleaseTarget</th>
                <th class="text-left">RemainHours</th>
                <th class="text-left">Status</th>
                <th class="text-left">UniqueLink</th>
                <th class="text-left">WorkDate</th>
                <th class="text-right">Version</th>                <th class="text-right">Effective Start DateTime</th>                <th class="text-right">Effective End DateTime</th>                <th class="text-left">Operation Type</th>                <th>Actions</th>            </tr>            </thead>            <tbody>
            @foreach (EdpsProjectManagement.Entities.BusinessEntities.Task Item in Model)
                        {                <tr>                    <td class="text-left">                        @Html.DisplayFor(modelItem => Item.Name)                    </td>                    <td class="text-left">
	                        @Html.DisplayFor(modelItem => Item.Iteration.Name)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem => Item.Project.Name)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem => Item.Repository.Name)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.ActualHours)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.Assignee)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.EstimateHours)
                    </td>
                    <td class="text-left">
	                        @(Item.ImportDateTime == DateTime.MinValue ? String.Empty : Item.ImportDateTime.ToShortDateString())
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.Label)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.ReleaseTarget)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.RemainHours)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.Status)
                    </td>
                    <td class="text-left">
	                        @Html.DisplayFor(modelItem =>Item.UniqueLink)
                    </td>
                    <td class="text-left">
	                        @(Item.WorkDate == DateTime.MinValue ? String.Empty : Item.WorkDate.ToShortDateString())
                    </td>
                <td class="text-right">                    @Html.DisplayFor(modelItem => Item.Version)                </td>                <td class="text-right">                    @Html.DisplayFor(modelItem => Item.EffectiveStartDateTime)                </td>                <td class="text-right">                    @Html.DisplayFor(modelItem => Item.EffectiveEndDateTime)                </td>                <td class="text-left">                    @Html.DisplayFor(modelItem => Item.OperationType)                </td>                <td>                        @Html.ActionLink(" ", "ViewOneHistoryVersion", new { systemId = Item.SystemId, Controller = "Task", area = ""}, new {@class="m_details", title="Details" })
                        </td>                </tr>            }            </tbody>        </table>    </div>    <div class="width_100 text-center m_left">        @Html.ActionLink("Back to List", "Index",null,new { @class="m_btn m_back"})    </div>    </div></div>