@model EdpsProjectManagement.Web.Models.BusinessEntities.TaskModel
@{    ViewBag.Title = "Details";    Layout  = "~/Views/Shared/_Layout.cshtml";}<div class="panel panel-default">    <div class="panel-body">        <div class="row">            <div class="col-md-6">	            <div class="form-group">                    <label class="col-md-4 from-title">Name</label>                    <div class="col-md-8 from-cont">@Html.DisplayFor(model => model.Name)</div>                </div>                <div class="form-group">                    <label class="col-md-4 from-title">Description</label>                    <div class="col-md-8 from-cont">@Html.DisplayFor(model => model.Description)</div>                </div>                 <div class="form-group">
	<label class="col-md-4 from-title">Iteration</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.Iteration.Name)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">Project</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.Project.Name)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">Repository</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.Repository.Name)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">ActualHours</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.ActualHours)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">Assignee</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.Assignee)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">EstimateHours</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.EstimateHours)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">ImportDateTime</label>
	                 <div class="col-md-8 from-cont">
		                 @(this.Model.ImportDateTime == DateTime.MinValue ? String.Empty : this.Model.ImportDateTime.ToShortDateString())
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">Label</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.Label)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">ReleaseTarget</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.ReleaseTarget)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">RemainHours</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.RemainHours)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">Status</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.Status)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">UniqueLink</label>
	                 <div class="col-md-8 from-cont">
		                 @Html.DisplayFor(model => model.UniqueLink)
	                 </div>
                 </div>
                 <div class="form-group">
	<label class="col-md-4 from-title">WorkDate</label>
	                 <div class="col-md-8 from-cont">
		                 @(this.Model.WorkDate == DateTime.MinValue ? String.Empty : this.Model.WorkDate.ToShortDateString())
	                 </div>
                 </div>
                 <div class="form-group">                     <label class="col-md-4 from-title" >Version</label>                     <div class="col-md-8 from-cont">                         @Html.DisplayFor(modelItem => modelItem.Version)                     </div>                 </div>                 <div class="form-group">                     <label class="col-md-4 from-title" >Modified User Name</label>                     <div class="col-md-8 from-cont">                         @Html.DisplayFor(modelItem => modelItem.ModifiedUserName)                     </div>                 </div>                 <div class="form-group">                     <label class="col-md-4 from-title" >Modified DateTime</label>                     <div class="col-md-8 from-cont">                         @Html.DisplayFor(modelItem => modelItem.ModifiedDateTime)                     </div>                 </div>                 <div class="form-group">                     <label class="col-md-4 from-title" >Operation Type</label>                     <div class="col-md-8 from-cont">                         @Html.DisplayFor(modelItem => modelItem.OperationType)                     </div>                 </div>                 <div class="form-group">                     <label class="col-md-4 from-title" >Effective Start DateTime</label>                     <div class="col-md-8 from-cont">                         @Html.DisplayFor(modelItem => modelItem.EffectiveStartDateTime)                     </div>                 </div>                 <div class="form-group">                     <label class="col-md-4 from-title" >Effective End DateTime</label>                     <div class="col-md-8 from-cont">                         @Html.DisplayFor(modelItem => modelItem.EffectiveEndDateTime)                     </div>                 </div>             </div>        </div>    </div>    <div class="width_100 text-center m_left">        @Html.ActionLink("Back to List", "ViewHistories",new{id=this.Model.Id},new { @class="m_btn m_back"})    </div></div>